{
  "name": "oncoprint-js",
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive JavaScript library for creating interactive oncoprint visualizations from MAF files and metadata",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "browser": "dist/oncoprint.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "browser": "./dist/oncoprint.umd.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "npm run build:watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run test && npm run typecheck && npm run build",
    "examples": "cd examples && python3 serve.py",
    "release": "npm version patch && npm publish"
  },
  "keywords": [
    "oncoprint",
    "bioinformatics",
    "cancer",
    "visualization",
    "d3",
    "genomics",
    "mutations",
    "maf",
    "typescript",
    "react",
    "data-visualization",
    "cancer-genomics",
    "interactive",
    "charts"
  ],
  "author": {
    "name": "Oncoprint.js Contributors",
    "url": "https://github.com/PoisonAlien/oncoprint-js/contributors"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/oncoprint-js.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/oncoprint-js/issues"
  },
  "homepage": "https://github.com/yourusername/oncoprint-js#readme"
}
